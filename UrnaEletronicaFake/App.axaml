<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="UrnaEletronicaFake.App"
             xmlns:local="using:UrnaEletronicaFake"
             RequestedThemeVariant="Light">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

   <Application.Resources>
    <ResourceDictionary>
        <ResourceDictionary.MergedDictionaries>
            <ResourceInclude Source="avares://UrnaEletronicaFake/Resources/Colors.axaml" />
            <ResourceInclude Source="avares://UrnaEletronicaFake/Resources/Themes.axaml" />
        </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
</Application.Resources>

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://UrnaEletronicaFake/Resources/Styles.axaml"/>
    </Application.Styles>
</Application>